	int hour;
	int day;
	int month;
	int year;
}

mgRegisterType(mp);

void mk_generic_func(class type, int iterations) {
	print("Classname = %q, Iterations = %i", type, interations);
	
	return (function) {
		void (typename(type) input) {
			for (int i = 0; i < const iterations; i++) {
				input.x += input.z;
			}
		}
	};
}

mp mp_time(int u) {
	mp newMP;
	
	newMP.sec = time_get_sec(u);
	newMP.min = time_get_min(u);
	newMP.hour = time_get_hour(u);
	newMP.day = time_get_day(u);
	newMP.month = time_get_month(u);
	newMP.year = time_get_year(u);
	
	return newMP;
}

